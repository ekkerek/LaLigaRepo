@page "/playerProfile/{Id:int}"
@using LA_LIGA_REKREATIVO.Client.Shared.PlayerProfile

<Tabs TabBarStyle="background-color: white;" Size="TabSize.Large" @bind-ActiveKey="@activeKey" Animated Centered>
    <TabPane Tab="Overview" Key="1">
        <PlayerOverview Id="Id"></PlayerOverview>
    </TabPane>
    <TabPane Tab="Matches" Key="2">
        <PlayerMatches Id="Id"></PlayerMatches>
    </TabPane>
</Tabs>

@code {
    [Parameter] public int Id { get; set; }

    string activeKey { get; set; } = "1";
}
